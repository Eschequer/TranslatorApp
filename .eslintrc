{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react-hooks/recommended", "prettier"],
  "plugins": ["react", "prettier", "autofix", "react-hooks"],
  "rules": {
    "semi": [2, "always"],
    "react/prop-types": 0,
    "autofix/no-debugger": "error",
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ]
    /*"no-unused-vars": "warn"*/
  }
}
